{
	"name": "Typing Sound with Lupin",
	"version": "1.0.2",
	"manifest_version": 2,
	"description": "フォーム入力時にルパンのサウンドを鳴らす",
	"icons": {
		"128": "icons/128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://*/*", "http://*/*", "file://*/*"],
			"js": [
				"scripts/jquery.min.js",
				"main.js"
			]
		}
	],
	"web_accessible_resources": [
		"sounds/enter.wav",
		"sounds/input.mp3"
	]
}
